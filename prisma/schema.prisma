datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/@prisma/client"
}

model User {
  id       String   @id @default(uuid())
  email    String   @unique
  password String
  name     String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
